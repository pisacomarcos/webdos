{{~#if parameters}}
	{{~#if parametersPath}}
		{{~#each parametersPath}}
      {{~#if ../onlyPathFirst}}
        {{~#if @first}}
          {{~{name}}}
        {{~/if}}
      {{~else}}
        {{~{name}}}{{#unless @last}},{{/unless}}
      {{~/if}}
		{{~/each}}
	{{~/if}}
  {{~#unless skipBody~}}
	{{~#if parametersBody}}
		{{~#if parametersPath}},{{/if}}
		{{~#with parametersBody}}
			{{~{name}}}
		{{~/with}}
		{{~#if parametersQuery}},{{/if}}
	{{~/if}}
  {{/unless~}}
	{{~#if parametersQuery}}
		{{~#if parametersPath}}{{~#if skipBody~}},{{else}}{{#unless parametersBody}},{{/unless}}{{/if}}{{/if~}}
		queryParams
	{{~/if}}
{{~/if}}
