{{~#if parameters}}
	{{~#if parametersPath}}

		{{#each parametersPath}}
      {{~#if ../onlyPathFirst}}
        {{~#if @first}}
          {{~{name}}}{{>isRequired}}: {{>type}}{{#if default}} = {{{default}}}{{/if}}
        {{~/if}}
      {{~else}}
        {{{name}}}{{>isRequired}}: {{>type}}{{#if default}} = {{{default}}}{{/if}}{{#unless @last}},
			  {{/unless}}
      {{/if}}
		{{~/each}}
	{{~/if}}
  {{~#unless skipBody~}}
    {{~#if parametersBody}}
      {{~#if parametersPath}},{{/if}}
      {{#with parametersBody}}
        {{{name}}}: {{>type}}
      {{~/with}}
      {{~#if parametersQuery}},{{/if}}
    {{~/if}}
  {{/unless~}}
	{{~#if parametersQuery}}
		{{~#if parametersPath}}{{~#if skipBody~}},{{else}}{{#unless parametersBody}},{{/unless}}{{/if}}{{/if}}
		{{#if codegen.params}}
			queryParams: {{codegen.params}} {{#unless isQueryParamsRequired}}= {}{{/unless}}
		{{~else~}}
			queryParams: {
				{{#each parametersQuery}}
					{{#ifdef description deprecated}}
					/**
					{{#if description}}
					 * {{{escapeComment description}}}
					{{/if}}
					{{#if deprecated}}
					 * @deprecated
					{{/if}}
					 */
					{{/ifdef}}
					{{{prop}}}{{>isRequired}}: {{>type}},
				{{/each}}
			} {{#unless isQueryParamsRequired}}= {}{{/unless}}
		{{~/if}}
	{{~/if}}
{{~/if}}
